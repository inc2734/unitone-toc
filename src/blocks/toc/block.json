{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "unitone-toc",
	"name": "unitone-toc/toc",
	"title": "Table of contents",
	"description": "Displays the table of contents of this page.",
	"keywords": [ "toc" ],
	"attributes": {
		"displayTitle": {
			"type": "boolean",
			"default": true
		},
		"title": {
			"type": "string",
			"source": "text",
			"selector": ".unitone-toc__title"
		},
		"headings": {
			"type": "string",
			"default": "h2,h3,h4"
		},
		"moveToBefore1stHeading": {
			"type": "boolean",
			"default": false
		},
		"id": {
			"type": "string"
		}
	},
	"styles": [
		{
			"name": "default",
			"label": "Default",
			"isDefault": true
		},
		{
			"name": "toc-1",
			"label": "1"
		},
		{
			"name": "toc-2",
			"label": "2"
		}
	],
	"selectors": {
		"root": ".unitone-toc"
	},
	"supports": {
		"className": false,
		"anchor": true,
		"color": {
			"background": true,
			"color": true,
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		},
		"unitone": {
			"padding": true
		}
	},
	"category": "unitone/component",
	"style": "file:./style-index.css",
	"editorStyle": "file:./index.css",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js"
}
